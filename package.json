{
  "name": "urdu-learning-app",
  "version": "1.0.0",
  "description": "Complete Urdu Language Learning Platform with Google TTS/STT",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node seedDatabase.js --reset",
    "seed:culture": "node scripts/seedCulture.js",
    "seed:context-words": "node scripts/seedContextWords.js",
    "seed:lessons": "node scripts/importLevelsFromCsv.js lessons_complete.csv --repeat-missing",
    "seed:lessons:first10": "node seedDatabase.js --reset-lessons-first10 --skip-culture --skip-context",
    "import:levels": "node scripts/importLevelsFromCsv.js urdu_learning_platform_complete_50_units.csv"
  },
  "dependencies": {
    "@google-cloud/speech": "^6.0.1",
    "@google-cloud/text-to-speech": "^5.0.1",
    "bcryptjs": "^3.0.3",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongoose": "^7.6.3",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
